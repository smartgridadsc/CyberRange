include includesied.mk
include make_config.mk

OBJ_DIR := build

FLAGS = -w -pthread #-lmysqlpp -lmysqlclient

all: $(MODELFILE) $(PROJECT_BINARY_NAME)

$(MODELFILE): $(ICDFILE)
	@echo "Generating $@ model..."
	java -jar $(MODELGEN) $(ICDFILE)

$(PROJECT_BINARY_NAME): $(PROJECT_SOURCES) $(LIBIEC)
	@echo "Compiling $@..."
	$(CXX) -o $@ $(PROJECT_SOURCES) $(LIBIEC) $(INCLUDES) $(FLAGS)

.PHONY = all

$(OBJ_DIR)/%.o: $(PROJECT_SOURCES)





